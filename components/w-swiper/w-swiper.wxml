<!--components/w-swiper/w-swiper.wxml-->
<swiper class="swiper swiper-child"     
        circular
        autoplay
        iterval="2000"
        indicator-dots
        indicator-active-color="#69ac76">
  <block wx:for="{{list}}" wx:key="index">
    <swiper-item class="swiper-item"> 
      <image src="{{item.image}}" mode="widthFix"></image>
    </swiper-item>
  </block>
</swiper>
